{
  "parser": "babel-eslint",
  "extends": ["prettier", "prettier/react"],
  "rules": {
    "semi": 0,
    "space-before-function-paren": "off",
    "no-console": "warn"
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 8,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["react"]
}
